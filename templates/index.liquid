<h2>{{ collections['raeggeton'].title }}</h2>

{% for product in collections['raeggeton'].products %}
    <div>
        <img src="{{ product.featured_image | img_url: '200x200' }}" alt="{{ product.title }}">
        <h3>{{ product.title }}</h3>
        <p>{{ product.price }}</p>
    </div>
  {{- product.title | link_to: product.url }}
{% endfor %}